# Alpaca API Configuration (required for backtesting and live trading)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_PAPER=true

# Anthropic API Configuration (optional for backtesting with use_ai=False)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AI Model Selection (optional - defaults to sonnet)
# Options: claude-opus-4-5-20251101, claude-sonnet-4-20250514, claude-haiku-4-20250514
CLAUDE_MODEL=claude-sonnet-4-20250514
